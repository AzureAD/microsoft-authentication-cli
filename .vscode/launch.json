    "version": "0.2.0",
    "configurations": [
        {
            "name": "auth az",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/Auth.CLI/bin/Debug/net5.0/osx-x64/Auth.CLI.dll",
            "args": ["az"],
            "cwd": "${workspaceFolder}/Auth.CLI",
            "stopAtEntry": true,
            "console": "internalConsole"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}